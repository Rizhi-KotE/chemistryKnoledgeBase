ui_menu_file_for_classificate_substance 
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
		ui_one_argument_command_class;
	=> nrel_main_idtf: 
		[Какому классу принадлежит вещество?] (* <- lang_ru;; *);
		[What is the class of substance?] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_classificate_substance _-> ._question_classificate_substance_instance
			(*
				_-> ui_arg_1;;
			*);;
		._question_classificate_substance_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос определения класса вещества: $ui_arg_1] 
		(* <- lang_ru;; *);
		[What is the class of $ui_arg_1] 
		(* <- lang_en;; *);;
